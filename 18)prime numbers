#include <stdio.h>  
int main()  
{  
    int ct=0,n=0,i=1,j=1,m;
    printf("enter no.of prime numbers ");
    while(n<m)  
    scanf("%d",&m);
    {  
        j=1;  
        ct=0;  
        while(j<=i)  
        {  
            if(i%j==0)  
            ct++;  
            j++;   
        }  
        if(ct==2)  
        {  
            printf("%d ",i);  
            n++;  
        }  
        i++;  
}  
}
